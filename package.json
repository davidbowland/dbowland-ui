{
  "name": "dbowland-gatsby",
  "version": "1.1.0",
  "private": true,
  "description": "dbowland.com gatsbyjs project",
  "author": "David Bowland",
  "engines": {
    "node": "^16.0.0"
  },
  "keywords": [
    "gatsby",
    "dbowland.com"
  ],
  "license": "ISC",
  "scripts": {
    "build": "gatsby build",
    "build:prod": "npm run clean && npm run build",
    "clean": "gatsby clean && rm -rf coverage deploy/temp && npm ci",
    "deploy": "git checkout master && git pull --ff-only && npm run build:prod && npm version $npm_config_type -m \"$npm_config_type version %s\" && node deploy/index.js && git push --no-verify",
    "lint": "prettier --write . && eslint --fix .",
    "start": "gatsby develop",
    "serve": "npm run build:prod && gatsby serve",
    "test": "jest --colors",
    "test:update-snapshots": "jest --colors -u",
    "workflow": "act -s DBOWLAND_SSH_USER -s DBOWLAND_SSH_PASS -s GITHUB_TOKEN"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "eslint --fix . && jest --colors"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --fix",
      "jest --colors --findRelatedTests"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@fontsource/fira-code": "^4.2.2",
    "@fontsource/lato": "^4.2.2",
    "@fontsource/rokkitt": "^4.2.2",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^3.3.1",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-htaccess": "^1.4.0",
    "gatsby-plugin-image": "^1.3.1",
    "gatsby-plugin-mdx": "^2.3.0",
    "gatsby-plugin-react-helmet": "^4.4.0",
    "gatsby-plugin-sass": "^4.3.0",
    "gatsby-plugin-sharp": "^3.3.1",
    "gatsby-plugin-sitemap": "^3.3.0",
    "gatsby-plugin-styled-components": "^4.4.0",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-transformer-sharp": "^3.3.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.32.11",
    "ssh2": "^0.8.9",
    "styled-components": "^5.2.3",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.23",
    "@types/react-helmet": "^6.1.1",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^1.4.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "react-test-renderer": "^17.0.2"
  }
}

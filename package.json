{
  "name": "dbowland-gatsby",
  "version": "1.0.0",
  "private": true,
  "description": "dbowland-gatsby",
  "author": "David Bowland",
  "engines": {
    "node": "^16.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "lint": "prettier --write .",
    "start": "gatsby develop",
    "serve": "gatsby serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "eslint --fix && jest --colors --collectCoverage"
    }
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown",
      "eslint --fix",
      "jest --colors --collectCoverage --findRelatedTests"
    ]
  },
  "dependencies": {
    "@fontsource/fira-code": "^4.2.2",
    "@fontsource/rokkitt": "^4.2.2",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^3.3.1",
    "gatsby-plugin-htaccess": "^1.4.0",
    "gatsby-plugin-image": "^1.3.1",
    "gatsby-plugin-mdx": "^2.3.0",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-sass": "^4.3.0",
    "gatsby-plugin-sharp": "^3.3.1",
    "gatsby-plugin-sitemap": "^3.3.0",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-transformer-sharp": "^3.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.32.11",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-react": "^7.23.2",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 80
      }
    }
  }
}

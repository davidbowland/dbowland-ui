{
  "name": "dbowland-ui",
  "version": "1.44.0",
  "private": true,
  "description": "dbowland.com gatsbyjs project",
  "author": "David Bowland",
  "engines": {
    "node": "^18.0.0"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidbowland/dbowland-ui.git"
  },
  "bugs": {
    "url": "https://github.com/davidbowland/dbowland-ui/issues"
  },
  "homepage": "https://github.com/davidbowland/dbowland-ui#readme",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rm -rf .cache coverage public && npm ci --legacy-peer-deps && gatsby clean",
    "deploy": "npm run build && ./scripts/deploy.sh",
    "lint": "prettier --write . && eslint --fix . --resolve-plugins-relative-to .",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install ; fi",
    "start": "gatsby develop",
    "serve": "npm run build && gatsby serve",
    "test": "jest --colors",
    "typecheck": "tsc --noEmit",
    "update": "ncu -u --doctor && npm audit fix --audit-level=none && npm run test && npm dedupe"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --fix --resolve-plugins-relative-to .",
      "jest --colors --findRelatedTests --coverageThreshold '{}' --passWithNoTests"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^4.4.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/fira-code": "^4.5.13",
    "@fontsource/roboto": "^4.5.8",
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "aws-amplify": "^5.0.22",
    "babel-plugin-styled-components": "^2.0.7",
    "crypto-browserify": "^3.12.0",
    "gatsby": "^5.8.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-image": "^3.8.0",
    "gatsby-plugin-mdx": "^5.8.0",
    "gatsby-plugin-react-helmet": "^6.8.0",
    "gatsby-plugin-sass": "^6.8.0",
    "gatsby-plugin-sharp": "^5.8.0",
    "gatsby-plugin-sitemap": "^6.8.0",
    "gatsby-plugin-styled-components": "^6.8.0",
    "gatsby-source-filesystem": "^5.8.0",
    "gatsby-transformer-sharp": "^5.8.0",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-is": "^18.2.0",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.9",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.21.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.30",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@types/styled-components": "^5.1.26",
    "babel-jest": "^29.5.0",
    "babel-preset-gatsby": "^3.8.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "prettier": "^2.8.7",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.0.5"
  }
}
